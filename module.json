{
  "id": "babele-translation-files-generator",
  "title": "[BTFG] Babele Translation Files Generator",
  "description": "Generate translation file for Babele by selecting a valid Compendium.",
  "authors": [
    {
      "name": "DjLeChuck",
      "email": "djlechuck@gmail.com",
      "url": "https://github.com/djlechuck/",
      "flags": {}
    }
  ],
  "version": "13.1.0",
  "compatibility": {
    "minimum": 13,
    "verified": 13.345
  },
  "esmodules": [
    "assets/btfg.js"
  ],
  "styles": [
    "assets/btfg.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "cs",
      "name": "Čeština",
      "path": "lang/cs.json"
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "lang/fr.json"
    },
    {
      "lang": "pl",
      "name": "Polski",
      "path": "lang/pl.json"
    }
  ],
  "url": "https://github.com/arbaletrier/babele",
  "manifest": "https://raw.githubusercontent.com/arbaletrier/babele/main/module.json",
  "download": "https://github.com/arbaletrier/babele/blob/main/module.zip",
  "flags": {
    "hotReload": {
      "extensions": [
        "css",
        "hbs",
        "json"
      ],
      "paths": [
        "lang/",
        "assets/",
        "templates/"
      ]
    }
  }
}
